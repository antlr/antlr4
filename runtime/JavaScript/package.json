{
  "name": "antlr4",
  "version": "4.12.0",
  "description": "JavaScript runtime for ANTLR4",
  "types": "src/antlr4/index.node.d.ts",
  "main": "cjs/index.web.js",
  "module": "esm/index.web.js",
  "exports": {
    ".": {
      "types": "./src/antlr4/index.node.d.ts",
      "node": "./esm/index.node.js",
      "import": "./esm/index.web.js",
      "require": "./cjs/index.web.js"
    }
  },
  "repository": "antlr/antlr4.git",
  "files": [
    "src/antlr4/*.d.ts",
    "esm/",
    "cjs/",
    "umd/",
    "README.md"
  ],
  "keywords": [
    "lexer",
    "parser",
    "antlr",
    "antlr4",
    "grammar"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/antlr/antlr4/issues"
  },
  "homepage": "https://github.com/antlr/antlr4",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/node": "^18.15.3",
    "c8": "^7.12.0",
    "eslint": "^8.36.0",
    "jasmine": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "del-cli": "^5.0.0",
    "rollup": "^3.20.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build-cjs": "babel --config-file ./babel.commonjs.config.js --extensions \".js\" --out-dir cjs ./src/antlr4",
    "build-esm": "babel --extensions \".js\" --out-dir esm ./src/antlr4",
    "build-umd": "rollup --config",
    "build": "npm run clean && npm run build-cjs && npm run build-esm && npm run build-umd",
    "clean-cjs": "del-cli cjs/*",
    "clean-esm": "del-cli esm/* !esm/package.json",
    "clean-umd": "del-cli umd/*",
    "clean": "npm run clean-cjs && npm run clean-esm && npm run clean-umd",
    "test": "jasmine",
    "coverage": "c8 jasmine",
    "lint": "eslint src/antlr4/"
  },
  "engines": {
    "node": ">=16"
  }
}
