TestType() ::= "Lexer"

Grammar ::= [
	"L": {<grammar("L")>}
]

Input() ::= <%
xaf
%>

Output() ::= <<
I
[@0,0:2='xaf',\<1>,1:0]
[@1,3:2='\<EOF>',\<-1>,1:3]<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
lexer grammar <grammarName>;
I : ~[ab \n] ~[ \ncd]* {<writeln("\"I\"")>} ;
WS : [ \n\u000D]+ -> skip ;
>>
