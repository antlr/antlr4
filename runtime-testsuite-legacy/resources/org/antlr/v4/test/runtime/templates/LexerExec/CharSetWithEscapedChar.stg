TestType() ::= "Lexer"

Grammar ::= [
	"L": {<grammar("L")>}
]

Input() ::= "- ] "

Output() ::= <<
DASHBRACK
DASHBRACK
[@0,0:0='-',\<1>,1:0]
[@1,2:2=']',\<1>,1:2]
[@2,4:3='\<EOF>',\<-1>,1:4]<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
lexer grammar <grammarName>;
DASHBRACK : [\\-\]]+ {<writeln("\"DASHBRACK\"")>} ;
WS : [ \n]+ -> skip ;
>>
