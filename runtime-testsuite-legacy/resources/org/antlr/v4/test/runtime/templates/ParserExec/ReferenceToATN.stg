/**
 * This is a regression test for antlr/antlr4#561 "Issue with parser
 * generation in 4.2.2"
 * https://github.com/antlr/antlr4/issues/561
 */

TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Rule() ::= "a"

grammar(grammarName) ::= <<
grammar <grammarName>;
a : (ID|ATN)* ATN? {<writeln("$text")>} ;
ID : 'a'..'z'+ ;
ATN : '0'..'9'+;
WS : (' '|'\n') -> skip ;
>>
