import "RecursiveLexerRuleRefWithWildcardPlus.stg"

TestType() ::= "Lexer"

Grammar ::= [
	"L": {<grammar("L")>}
]

// stuff on end of comment matches another rule
Input() ::= <<
/* ick */
/* /* */
/* /*nested*/ */<\n>
>>

Output() ::= <<
[@0,0:8='/* ick */',\<1>,1:0]
[@1,9:9='\n',\<2>,1:9]
[@2,10:34='/* /* */\n/* /*nested*/ */',\<1>,2:0]
[@3,35:35='\n',\<2>,3:16]
[@4,36:35='\<EOF>',\<-1>,4:0]<\n>
>>

Errors() ::= ""
