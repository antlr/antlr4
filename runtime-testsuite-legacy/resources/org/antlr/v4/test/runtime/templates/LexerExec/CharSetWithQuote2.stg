TestType() ::= "Lexer"

Grammar ::= [
	"L": {<grammar("L")>}
]

Input() ::= <%
b"\a
%>

Output() ::= <<
A
[@0,0:3='b"\a',\<1>,1:0]
[@1,4:3='\<EOF>',\<-1>,1:4]<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
lexer grammar <grammarName>;
A : ["\\\\ab]+ {<writeln("\"A\"")>} ;
WS : [ \n\t]+ -> skip ;
>>
