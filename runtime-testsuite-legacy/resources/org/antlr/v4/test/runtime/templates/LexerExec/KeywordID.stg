TestType() ::= "Lexer"

Grammar ::= [
	"L": {<grammar("L")>}
]

Input() ::= <%
end eend ending a
%>

Output() ::= <<
[@0,0:2='end',\<1>,1:0]
[@1,3:3=' ',\<3>,1:3]
[@2,4:7='eend',\<2>,1:4]
[@3,8:8=' ',\<3>,1:8]
[@4,9:14='ending',\<2>,1:9]
[@5,15:15=' ',\<3>,1:15]
[@6,16:16='a',\<2>,1:16]
[@7,17:16='\<EOF>',\<-1>,1:17]<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
lexer grammar <grammarName>;
KEND : 'end' ; // has priority
ID : 'a'..'z'+ ;
WS : (' '|'\n')+;
>>
