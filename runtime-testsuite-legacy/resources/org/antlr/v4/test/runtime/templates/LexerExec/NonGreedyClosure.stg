TestType() ::= "Lexer"

Grammar ::= [
	"L": {<grammar("L")>}
]

Input() ::= <<
//blah
//blah<\n>
>>

Output() ::= <<
[@0,0:6='//blah\n',\<1>,1:0]
[@1,7:13='//blah\n',\<1>,2:0]
[@2,14:13='\<EOF>',\<-1>,3:0]<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
lexer grammar <grammarName>;
CMT : '//' .*? '\n' CMT*?;
WS : (' '|'\t')+;
>>
