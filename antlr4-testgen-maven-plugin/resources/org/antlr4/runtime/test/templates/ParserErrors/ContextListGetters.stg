/**
 * Regression test for "Getter for context is not a list when it should be".
 * https://github.com/antlr/antlr4/issues/19
 */

TestType() ::= "Parser"

Options ::= [
	"Debug": true
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Input() ::= "abab"

Rule() ::= "s"

Output() ::= <<
abab<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
grammar <grammarName>;
@parser::members{
<DeclareContextListGettersFunction()>
}
s : (a | b)+;
a : 'a' {<write("'a'")>};
b : 'b' {<write("'b'")>};
>>
