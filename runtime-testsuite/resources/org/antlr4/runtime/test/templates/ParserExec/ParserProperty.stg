/*
 * This is a regression test for antlr/antlr4#561 "Issue with parser
 * generation in 4.2.2"
 * https://github.com/antlr/antlr4/issues/561
 */

TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Input() ::= "abc"

Rule() ::= "a"

Output() ::= <<
valid<\n>
>>

Errors() ::= ""

grammar(grammarName) ::= <<
grammar <grammarName>;
<ParserPropertyMember()>
a : {$parser.Property()}? ID {<writeln("\"valid\"")>}
  ;
ID : 'a'..'z'+ ;
WS : (' '|'\n') -> skip ;
>>
