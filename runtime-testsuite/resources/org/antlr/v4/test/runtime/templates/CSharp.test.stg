	public override void ExitA(TParser.AContext ctx) {
		if (ctx.ChildCount==2) {
			Output.Write("{0} {1} {2}",ctx.b(0).Start.Text,
				ctx.b(1).Start.Text,ctx.b()[0].Start.Text);
		} else
			Output.WriteLine(ctx.b(0).Start.Text);
	}
}
}
