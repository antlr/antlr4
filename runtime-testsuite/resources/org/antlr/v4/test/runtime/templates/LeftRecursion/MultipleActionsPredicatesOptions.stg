/*
 * This is a regression test for antlr/antlr4#625 "Duplicate action breaks
 * operator precedence"
 * https://github.com/antlr/antlr4/issues/625
 */

TestType() ::= "Parser"

Options ::= [
	"Debug": false
]

Grammar ::= [
	"T": {<grammar("T")>}
]

Rule() ::= "s"

grammar(grammarName) ::= <<
grammar <grammarName>;
s @after {<ToStringTree("$ctx"):writeln()>} : e ;
e : a=e op=('*'|'/') b=e  {}{<True()>}?
  | a=e op=('+'|'-') b=e  {}\<p=3>{<True()>}?\<fail='Message'>
  | INT {}{}
  | '(' x=e ')' {}{}
  ;
INT : '0'..'9'+ ;
WS : (' '|'\n') -> skip ;
>>
