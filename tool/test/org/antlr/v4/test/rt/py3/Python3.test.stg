    def exitA(self, ctx):
        if ctx.getChildCount()==2:
            print(ctx.INT(0).symbol.text + ' ' + ctx.INT(1).symbol.text + ' ' + str_list(ctx.INT()))
        else:
            print(str(ctx.ID().symbol))

